{
  "canisters": {
    "bigmap_ui": {
      "frontend": {
        "entrypoint": "src/public/App.tsx"
      },
      "source": [
        "src/assets",
        "dist/bigmap_ui/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.6.6",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
