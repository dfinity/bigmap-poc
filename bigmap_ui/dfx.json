{
  "canisters": {
    "tsreact_v2": {
      "main": "src/tsreact_v2/main.mo",
      "type": "motoko"
    },
    "bigmap_ui": {
      "dependencies": [
        "tsreact_v2"
      ],
      "frontend": {
        "entrypoint": "src/bigmap_ui/public/App.tsx"
      },
      "source": [
        "src/bigmap_ui/assets",
        "dist/bigmap_ui/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.6.4",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "tungsten": {
      "providers": [
        "https://gw.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}